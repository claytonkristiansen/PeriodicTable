<Window x:Class="PeriodicTable.Element"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PeriodicTable"
        mc:Ignorable="d"
        Title="Element" Height="450" Width="800"
        KeyDown="Window_KeyDown">
    <Grid Background="AliceBlue">
        <TextBox Name="ElementName" HorizontalAlignment="Left" Height="23" Margin="65,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" BorderThickness="0" Background="AliceBlue"/>
        <WebBrowser Name="elementWebBrowser" Navigating="ElementWebBrowser_Navigating" Margin="250,0,0,0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
    </Grid>
</Window>
